{
  "properties": {
    "listing_id": {
      "type": "keyword"
    },
    "neighborhood_id": {
      "type": "keyword"
    },
    "property_type": {
      "type": "keyword",
      "normalizer": "lowercase_normalizer"
    },
    "price": {
      "type": "float"
    },
    "bedrooms": {
      "type": "short"
    },
    "bathrooms": {
      "type": "half_float"
    },
    "square_feet": {
      "type": "integer"
    },
    "year_built": {
      "type": "short"
    },
    "lot_size": {
      "type": "integer"
    },
    "address": {
      "type": "object",
      "properties": {
        "street": {
          "type": "text",
          "analyzer": "address_analyzer",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        },
        "city": {
          "type": "keyword",
          "normalizer": "lowercase_normalizer"
        },
        "state": {
          "type": "keyword"
        },
        "zip_code": {
          "type": "keyword"
        },
        "location": {
          "type": "geo_point"
        }
      }
    },
    "neighborhood": {
      "type": "object",
      "properties": {
        "id": {
          "type": "keyword"
        },
        "name": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword"
            }
          }
        },
        "walkability_score": {
          "type": "byte"
        },
        "school_rating": {
          "type": "half_float"
        }
      }
    },
    "description": {
      "type": "text",
      "analyzer": "property_analyzer",
      "fields": {
        "keyword": {
          "type": "keyword",
          "ignore_above": 256
        }
      }
    },
    "features": {
      "type": "keyword",
      "normalizer": "lowercase_normalizer"
    },
    "status": {
      "type": "keyword"
    },
    "listing_date": {
      "type": "date"
    },
    "last_updated": {
      "type": "date"
    },
    "days_on_market": {
      "type": "integer"
    },
    "price_per_sqft": {
      "type": "float"
    },
    "hoa_fee": {
      "type": "float"
