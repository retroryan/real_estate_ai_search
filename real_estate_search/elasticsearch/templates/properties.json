          "type": "byte"
        },
        "type": {
          "type": "keyword"
        }
      }
    },
    "virtual_tour_url": {
      "type": "keyword",
      "index": false,
      "doc_values": false
    },
    "images": {
      "type": "keyword",
      "index": false,
      "doc_values": false
    },
    "mls_number": {
      "type": "keyword"
